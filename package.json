{
  "name": "secondBrain",
  "type": "module",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "devalue": ">=5.6.2",
      "tar": ">=7.5.3",
      "diff": ">=8.0.3"
    }
  },
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "cleanup": "rm -rf .nuxt .output node_modules/.cache && git worktree list --porcelain | grep '^worktree' | grep -v \"$(pwd)$\" | cut -d' ' -f2 | xargs -I {} git worktree remove --force {} 2>/dev/null; rm -rf .worktrees",
    "build:excalidraw": "npx tsx scripts/build-excalidraw.ts",
    "build:og": "npx tsx scripts/build-og-images.ts",
    "generate:embeddings": "npx tsx scripts/generate-embeddings.ts",
    "prebuild": "pnpm generate:embeddings",
    "build": "pnpm build:excalidraw && pnpm build:og && nuxt build",
    "dev": "nuxt dev",
    "typecheck": "nuxt typecheck",
    "generate": "nuxt generate",
    "lint": "oxlint && eslint . && markdownlint-cli2 'content/**/*.md'",
    "lint:fix": "oxlint --fix && eslint . --fix && markdownlint-cli2 --fix 'content/**/*.md'",
    "lint:ox": "oxlint",
    "lint:es": "eslint .",
    "lint:md": "markdownlint-cli2 'content/**/*.md'",
    "preview": "nuxt preview",
    "test:unit": "vitest run --project unit",
    "test:unit:cov": "vitest run --project unit --coverage",
    "test:integration": "vitest run --project integration",
    "test:component": "vitest run --project component",
    "test:component:ui": "vitest --project component --ui",
    "test": "vitest run --project unit --project integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:mutation": "stryker run",
    "prepare": "husky",
    "ralph": "./ralph/ralph.sh",
    "fix-e2e": "./ralph/fix-e2e.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@huggingface/transformers": "^3.8.1",
    "@nuxt/content": "^3.10.0",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/ui": "4.3.0",
    "@portaljs/remark-wiki-link": "^1.2.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/d3": "^7.4.3",
    "@vueuse/core": "^14.2.0",
    "@vueuse/nuxt": "^14.2.0",
    "@vueuse/router": "^14.2.0",
    "d3": "^7.9.0",
    "d3-array": "^3.2.4",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-zoom": "^3.0.0",
    "eslint": "^9.39.2",
    "fuse.js": "^7.1.0",
    "h3": "^1.15.5",
    "mermaid": "^11.12.2",
    "nuxt": "^4.2.2",
    "satori": "^0.18.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/markdown": "^7.5.1",
    "@iconify-json/lucide": "^1.2.82",
    "@nuxt/a11y": "1.0.0-alpha.1",
    "@nuxt/kit": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@playwright/test": "^1.57.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/d3-array": "^3.2.2",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-zoom": "^3.0.8",
    "@types/node": "^25.0.3",
    "@typescript-eslint/parser": "^8.32.1",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vite-pwa/nuxt": "^1.1.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lucide-static": "^0.562.0",
    "markdownlint-cli2": "^0.20.0",
    "nitropack": "^2.12.9",
    "oxlint": "^1.0.0",
    "playwright": "^1.57.0",
    "typescript-eslint": "^8.32.1",
    "unplugin-auto-import": "^20.3.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "vitest-browser-vue": "^1.1.0",
    "vue": "^3.5.27",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.6.4",
    "yaml": "^2.8.2"
  }
}
